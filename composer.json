{
    "name": "waltertamboer/phpmysupport",
    "description": "This project sets up a support system to help customers.",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "ext-dom": "*",
        "doctrine/cache": "^2.1",
        "doctrine/migrations": "^3.4",
        "doctrine/orm": "^2.11",
        "laminas/laminas-config-aggregator": "^1.7",
        "laminas/laminas-crypt": "^3.6",
        "laminas/laminas-diactoros": "^2.8",
        "laminas/laminas-servicemanager": "^3.10",
        "mezzio/mezzio": "^3.10",
        "mezzio/mezzio-authentication": "^1.4",
        "mezzio/mezzio-authentication-session": "^1.3",
        "mezzio/mezzio-authorization": "^1.4",
        "mezzio/mezzio-authorization-rbac": "^1.3",
        "mezzio/mezzio-cors": "^1.3",
        "mezzio/mezzio-csrf": "^1.5",
        "mezzio/mezzio-fastroute": "^3.5",
        "mezzio/mezzio-flash": "^1.3",
        "mezzio/mezzio-problem-details": "^1.6",
        "mezzio/mezzio-session": "^1.6",
        "mezzio/mezzio-session-ext": "^1.12",
        "mezzio/mezzio-twigrenderer": "^2.9",
        "ramsey/uuid": "^4.2",
        "ramsey/uuid-doctrine": "^1.8",
        "symfony/console": "^6.0"
    },
    "require-dev": {
        "filp/whoops": "^2.7.1",
        "mezzio/mezzio-tooling": "^2.5",
        "phpunit/phpunit": "^9.5.11"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true,
            "laminas/laminas-component-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    }
}
